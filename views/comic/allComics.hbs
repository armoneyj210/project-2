<h1>Marvel Vs. DC</h1>
<a href="/comic/new"> <button>Create Comic</button></a>
<ul>
    {{#each allComics}}
    <a href="/comic/{{_id}}">{{name}}</a>
    <h4>Universe:</h4>
    <h3>{{universe}}</h3>
    <h4>Description:</h4>
    <h3>{{description}}</h3>
    <h4>First Appearance:</h4>
    <h3>{{firstAppearance}}</h3>
    <h3>{{pic}}</h3>

    {{/each}}
</ul>